{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-confusing-arrow": 0,
    "react/jsx-quotes": 0,
    "no-underscore-dangle": 0,
    "jsx-quotes": [2, "prefer-double"],
    "no-restricted-syntax": 0,
    "react/no-danger": 0,
    "react/prefer-stateless-function": 0,
    "global-require": 0
  }
}
